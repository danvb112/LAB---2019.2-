N, M = input().split()
M = int(M)
ping = []
cabo_entre_ilhas = []
for i in range(M):
    U, V, P = input().split()
    ping.append(P)
    cabo_entre_ilhas.append([U,V])
    print(ping)
    print(cabo_entre_ilhas)

